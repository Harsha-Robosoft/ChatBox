# Uncomment the next line to define a global platform for your project
# platform :ios, '9.0'

target 'ChatBox' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!


# Firebase

  pod 'Firebase/Core'
  pod 'Firebase/Auth'
  pod 'Firebase/Database'
  pod 'Firebase/Storage'


# GoogleSignIn

  pod 'GoogleSignIn'


# Facebook login

  pod 'FBSDKLoginKit'
  pod 'FBSDKCoreKit'

  pod 'MessageKit'
  pod 'JGProgressHUD'
  pod 'RealmSwift'
  pod 'SDWebImage'
    
end
